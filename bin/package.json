{
  "name": "lox2mqtt",
  "version": "0.4.1",
  "description": "Loxone Miniserver communicating with MQTT server",
  "main": "lox2mqtt.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "dev": "node lox2mqtt.js",
    "start": "PM2_HOME=$(pwd)/.pm2 pm2 start lox2mqtt.config.js --watch",
    "restart": "PM2_HOME=$(pwd)/.pm2 pm2 restart lox2mqtt.config.js --watch",
    "stop": "PM2_HOME=$(pwd)/.pm2 pm2 stop lox2mqtt.config.js",
    "delete": "PM2_HOME=$(pwd)/.pm2 pm2 delete lox2mqtt.config.js",
    "kill": "PM2_HOME=$(pwd)/.pm2 pm2 kill",
    "list": "PM2_HOME=$(pwd)/.pm2 pm2 list",
    "logs": "PM2_HOME=$(pwd)/.pm2 pm2 logs",
    "status": "PM2_HOME=$(pwd)/.pm2 pm2 status"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nufke/LoxBerry-Plugin-Lox2MQTT.git"
  },
  "keywords": [
    "Loxone",
    "API",
    "MQTT"
  ],
  "author": "Martin Barnasconi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nufke/LoxBerry-Plugin-Lox2MQTT/issues"
  },
  "homepage": "https://github.com/nufke/LoxBerry-Plugin-Lox2MQTT/",
  "dependencies": {
    "mqtt": "^4.3.7",
    "lodash": "4.17.21",
    "loxberry-logger": "git+https://github.com/nufke/loxberry-logger.git#9d1a7f0",
    "node-lox-structure-file": "git+https://github.com/nufke/node-lox-structure-file.git#0db160c",
    "node-lox-ws-api": "^0.4.5",
    "pm2": "^5.3.0"
  },
  "devDependencies": {
    "vows": ">=0.8.1"
  }
}
